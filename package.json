{
  "name": "task2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run prep:neu-dev && concurrently -k \"npm:dev:vite\" \"npm:dev:neu\"",
    "build": "npm run build:vite && npm run build:neu",
    "dev:neu": "neu run --frontend-lib-dev",
    "build:neu": "neu build",
    "prep:neu-dev": "node -e \"const fs=require('fs');fs.mkdirSync('www/build/icons',{recursive:true});fs.copyFileSync('www/public/icons/appIcon.png','www/build/icons/appIcon.png');\"",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview": "vite preview",
    "postinstall": "neu update"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mantine/core": "^8.3.15",
    "@mantine/hooks": "^8.3.15",
    "@mantine/modals": "^8.3.15",
    "@mantine/notifications": "^8.3.15",
    "@tabler/icons-react": "^3.36.1",
    "dayjs": "^1.11.19",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "uuid": "^13.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^9.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "vite": "^5.4.0"
  }
